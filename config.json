{
    "export_dir": "\\\\TUIMAC\\Users\\Administrator\\OneDrive\\ドキュメント\\monitor_mem\\log\\host1",
    "metrics": ["create_time", "pid", "name", "username"],
    "max_thread": "50",
    "log_config": {
        "version": 1,
        "disable_existing_loggers": "True",
        "formatters": {
            "file": {
                "format": "[%(levelname)s] - %(asctime)s - Source log file: %(pathname)s line: %(lineno)s %(message)s"
            }
        },
        "handlers": {
            "file": {
                "level": "INFO",
                "class": "logging.handlers.RotatingFileHandler",
                "filename": "main.log",
                "maxBytes": 10485760,
                "formatter": "file"
            }
        },
        "loggers": {
            "main": {
                "handlers": ["file"],
                "level": "INFO",
                "propagate": "True"
            }
        }
    }
}